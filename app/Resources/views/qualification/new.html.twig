{% extends 'base.html.twig' %}

{% block body %}
<div class="container">
    <div class="panel">
        <div class="panel-body">
            <h1>Nouvelle Qualification</h1>

            {{ form_start(form) }}
            {{ form_widget(form) }}
            <div class="row">
                <div class="col-md-6">
                    <input type="submit" class="btn btn-success" value="Create"/>
                </div>
                <div class="col-md-6">
                    <a href="{{ path('qualification_index') }}" class="btn btn-info" >Back to the list</a>
                </div>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
</div>
{% endblock %}
